    <bpmn:callActivity id="CallActivity_10" name="Раздел 10: Выборочные проверки" calledElement="Process_Section_10">
      <bpmn:incoming>F6</bpmn:incoming>
      <bpmn:outgoing>F8</bpmn:outgoing>
    </bpmn:callActivity>
    <bpmn:callActivity id="CallActivity_11" name="Раздел 11: Плановые аудиты" calledElement="Process_Section_11">
      <bpmn:incoming>F7</bpmn:incoming>
      <bpmn:outgoing>F9</bpmn:outgoing>
