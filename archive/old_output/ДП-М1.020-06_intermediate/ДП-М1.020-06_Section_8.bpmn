<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL" 
                  xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" 
                  xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" 
                  xmlns:di="http://www.omg.org/spec/DD/20100524/DI" 
                  id="Definitions_Section_8" 
                  targetNamespace="http://bpmn.io/schema/bpmn">
  
  <bpmn:collaboration id="Collaboration_Section_8">
    <bpmn:participant id="Participant_Section_8" name="Раздел 8: Мониторинг аэродромной инфраструктуры" processRef="Process_Section_8" />
    <bpmn:participant id="Participant_Contractor" name="Контрагент (аэродром)" />
    <bpmn:messageFlow id="MsgFlow_Request" sourceRef="Task_8_3_6_2_Request" targetRef="Participant_Contractor" />
    <bpmn:messageFlow id="MsgFlow_Response" sourceRef="Participant_Contractor" targetRef="Task_8_3_6_2_Receive" />
  </bpmn:collaboration>
  
  <bpmn:process id="Process_Section_8" name="Мониторинг аэродромов" isExecutable="true">
    
    <bpmn:laneSet id="LaneSet_8">
      <bpmn:lane id="Lane_8_Crew" name="Экипажи / EFB">
        <bpmn:flowNodeRef>Start_8</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_8_3_5</bpmn:flowNodeRef>
      </bpmn:lane>
      
      <bpmn:lane id="Lane_8_Risk_Engineer" name="Инженер по анализу рисков">
        <bpmn:flowNodeRef>Task_8_3_6_1</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Gateway_8_Risk</bpmn:flowNodeRef>
      </bpmn:lane>
      
      <bpmn:lane id="Lane_8_Manager" name="Менеджер ООНО">
        <bpmn:flowNodeRef>Task_8_3_6_2_Request</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_8_3_6_2_Receive</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_8_3_6_3</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_8_3_8</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>End_8_Normal</bpmn:flowNodeRef>
      </bpmn:lane>
      
      <bpmn:lane id="Lane_8_Emergency" name="Действия при красном уровне">
        <bpmn:flowNodeRef>Task_8_3_6_4</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_8_3_7</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>End_8_Red</bpmn:flowNodeRef>
      </bpmn:lane>
    </bpmn:laneSet>
    
    <!-- START -->
    <bpmn:startEvent id="Start_8" name="Ежедневный сбор данных" isInterrupting="false">
      <bpmn:outgoing>Flow_8_1</bpmn:outgoing>
      <bpmn:timerEventDefinition />
    </bpmn:startEvent>
    
    <!-- TASKS -->
    <bpmn:userTask id="Task_8_3_5" name="Сбор данных о состоянии аэродромов (EFB)">
      <bpmn:incoming>Flow_8_1</bpmn:incoming>
      <bpmn:outgoing>Flow_8_2</bpmn:outgoing>
    </bpmn:userTask>
    
    <bpmn:serviceTask id="Task_8_3_6_1" name="Ранжирование аэропортов по рискам (ежемесячно)">
      <bpmn:incoming>Flow_8_2</bpmn:incoming>
      <bpmn:outgoing>Flow_8_3</bpmn:outgoing>
    </bpmn:serviceTask>
    
    <bpmn:exclusiveGateway id="Gateway_8_Risk" name="Уровень риска?">
      <bpmn:incoming>Flow_8_3</bpmn:incoming>
      <bpmn:outgoing>Flow_8_Red</bpmn:outgoing>
      <bpmn:outgoing>Flow_8_Normal</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    
    <!-- RED RISK PATH -->
    <bpmn:userTask id="Task_8_3_6_4" name="Незамедлительные действия (КРАСНЫЙ уровень)">
      <bpmn:incoming>Flow_8_Red</bpmn:incoming>
      <bpmn:outgoing>Flow_8_Emergency</bpmn:outgoing>
    </bpmn:userTask>
    
    <bpmn:userTask id="Task_8_3_7" name="Реализация корректирующих мероприятий">
      <bpmn:incoming>Flow_8_Emergency</bpmn:incoming>
      <bpmn:outgoing>Flow_8_End_Red</bpmn:outgoing>
    </bpmn:userTask>
    
    <bpmn:endEvent id="End_8_Red" name="Риск снижен">
      <bpmn:incoming>Flow_8_End_Red</bpmn:incoming>
    </bpmn:endEvent>
    
    <!-- NORMAL PATH -->
    <bpmn:sendTask id="Task_8_3_6_2_Request" name="Информирование агента о необходимости проверки">
      <bpmn:incoming>Flow_8_Normal</bpmn:incoming>
      <bpmn:outgoing>Flow_8_4</bpmn:outgoing>
    </bpmn:sendTask>
    
    <bpmn:receiveTask id="Task_8_3_6_2_Receive" name="Получение информации от агента">
      <bpmn:incoming>Flow_8_4</bpmn:incoming>
      <bpmn:outgoing>Flow_8_5</bpmn:outgoing>
    </bpmn:receiveTask>
    
    <bpmn:userTask id="Task_8_3_6_3" name="Выездная проверка и оценка рисков на совещании SAG">
      <bpmn:incoming>Flow_8_5</bpmn:incoming>
      <bpmn:outgoing>Flow_8_6</bpmn:outgoing>
    </bpmn:userTask>
    
    <bpmn:userTask id="Task_8_3_8" name="Доведение информации до ДУБП">
      <bpmn:incoming>Flow_8_6</bpmn:incoming>
      <bpmn:outgoing>Flow_8_End_Normal</bpmn:outgoing>
    </bpmn:userTask>
    
    <bpmn:endEvent id="End_8_Normal" name="Цикл мониторинга завершен">
      <bpmn:incoming>Flow_8_End_Normal</bpmn:incoming>
    </bpmn:endEvent>
    
    <!-- FLOWS -->
    <bpmn:sequenceFlow id="Flow_8_1" sourceRef="Start_8" targetRef="Task_8_3_5" />
    <bpmn:sequenceFlow id="Flow_8_2" sourceRef="Task_8_3_5" targetRef="Task_8_3_6_1" />
    <bpmn:sequenceFlow id="Flow_8_3" sourceRef="Task_8_3_6_1" targetRef="Gateway_8_Risk" />
    <bpmn:sequenceFlow id="Flow_8_Red" name="КРАСНЫЙ" sourceRef="Gateway_8_Risk" targetRef="Task_8_3_6_4" />
    <bpmn:sequenceFlow id="Flow_8_Normal" name="Допустимый" sourceRef="Gateway_8_Risk" targetRef="Task_8_3_6_2_Request" />
    <bpmn:sequenceFlow id="Flow_8_Emergency" sourceRef="Task_8_3_6_4" targetRef="Task_8_3_7" />
    <bpmn:sequenceFlow id="Flow_8_End_Red" sourceRef="Task_8_3_7" targetRef="End_8_Red" />
    <bpmn:sequenceFlow id="Flow_8_4" sourceRef="Task_8_3_6_2_Request" targetRef="Task_8_3_6_2_Receive" />
    <bpmn:sequenceFlow id="Flow_8_5" sourceRef="Task_8_3_6_2_Receive" targetRef="Task_8_3_6_3" />
    <bpmn:sequenceFlow id="Flow_8_6" sourceRef="Task_8_3_6_3" targetRef="Task_8_3_8" />
    <bpmn:sequenceFlow id="Flow_8_End_Normal" sourceRef="Task_8_3_8" targetRef="End_8_Normal" />
    
  </bpmn:process>
  
  <!-- VISUALIZATION -->
  <bpmndi:BPMNDiagram id="Diagram_Section_8">
    <bpmndi:BPMNPlane id="Plane_8" bpmnElement="Collaboration_Section_8">
      
      <bpmndi:BPMNShape id="Participant_Section_8_di" bpmnElement="Participant_Section_8" isHorizontal="true">
        <dc:Bounds x="160" y="80" width="2200" height="800" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Lane_8_Crew_di" bpmnElement="Lane_8_Crew" isHorizontal="true">
        <dc:Bounds x="190" y="80" width="2170" height="200" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Lane_8_Risk_Engineer_di" bpmnElement="Lane_8_Risk_Engineer" isHorizontal="true">
        <dc:Bounds x="190" y="280" width="2170" height="200" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Lane_8_Manager_di" bpmnElement="Lane_8_Manager" isHorizontal="true">
        <dc:Bounds x="190" y="480" width="2170" height="200" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Lane_8_Emergency_di" bpmnElement="Lane_8_Emergency" isHorizontal="true">
        <dc:Bounds x="190" y="680" width="2170" height="200" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Participant_Contractor_di" bpmnElement="Participant_Contractor" isHorizontal="true">
        <dc:Bounds x="160" y="1000" width="2200" height="100" />
      </bpmndi:BPMNShape>
      
      <!-- Start -->
      <bpmndi:BPMNShape id="Start_8_di" bpmnElement="Start_8">
        <dc:Bounds x="252" y="162" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="234" y="205" width="72" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <!-- Tasks -->
      <bpmndi:BPMNShape id="Task_8_3_5_di" bpmnElement="Task_8_3_5">
        <dc:Bounds x="340" y="140" width="140" height="80" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Task_8_3_6_1_di" bpmnElement="Task_8_3_6_1">
        <dc:Bounds x="530" y="340" width="140" height="80" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Gateway_8_Risk_di" bpmnElement="Gateway_8_Risk" isMarkerVisible="true">
        <dc:Bounds x="725" y="355" width="50" height="50" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="712" y="412" width="76" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <!-- Red Path -->
      <bpmndi:BPMNShape id="Task_8_3_6_4_di" bpmnElement="Task_8_3_6_4">
        <dc:Bounds x="830" y="740" width="140" height="80" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Task_8_3_7_di" bpmnElement="Task_8_3_7">
        <dc:Bounds x="1020" y="740" width="140" height="80" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="End_8_Red_di" bpmnElement="End_8_Red">
        <dc:Bounds x="1212" y="762" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="1195" y="805" width="70" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <!-- Normal Path -->
      <bpmndi:BPMNShape id="Task_8_3_6_2_Request_di" bpmnElement="Task_8_3_6_2_Request">
        <dc:Bounds x="830" y="540" width="140" height="80" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Task_8_3_6_2_Receive_di" bpmnElement="Task_8_3_6_2_Receive">
        <dc:Bounds x="1020" y="540" width="140" height="80" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Task_8_3_6_3_di" bpmnElement="Task_8_3_6_3">
        <dc:Bounds x="1210" y="540" width="140" height="80" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Task_8_3_8_di" bpmnElement="Task_8_3_8">
        <dc:Bounds x="1400" y="540" width="140" height="80" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="End_8_Normal_di" bpmnElement="End_8_Normal">
        <dc:Bounds x="1592" y="562" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="1575" y="605" width="70" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <!-- Edges -->
      <bpmndi:BPMNEdge id="Flow_8_1_di" bpmnElement="Flow_8_1">
        <di:waypoint x="288" y="180" />
        <di:waypoint x="340" y="180" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_8_2_di" bpmnElement="Flow_8_2">
        <di:waypoint x="480" y="180" />
        <di:waypoint x="505" y="180" />
        <di:waypoint x="505" y="380" />
        <di:waypoint x="530" y="380" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_8_3_di" bpmnElement="Flow_8_3">
        <di:waypoint x="670" y="380" />
        <di:waypoint x="725" y="380" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_8_Red_di" bpmnElement="Flow_8_Red">
        <di:waypoint x="750" y="405" />
        <di:waypoint x="750" y="780" />
        <di:waypoint x="830" y="780" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="720" y="593" width="58" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_8_Normal_di" bpmnElement="Flow_8_Normal">
        <di:waypoint x="750" y="405" />
        <di:waypoint x="750" y="580" />
        <di:waypoint x="830" y="580" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="710" y="493" width="68" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_8_Emergency_di" bpmnElement="Flow_8_Emergency">
        <di:waypoint x="970" y="780" />
        <di:waypoint x="1020" y="780" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_8_End_Red_di" bpmnElement="Flow_8_End_Red">
        <di:waypoint x="1160" y="780" />
        <di:waypoint x="1212" y="780" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_8_4_di" bpmnElement="Flow_8_4">
        <di:waypoint x="970" y="580" />
        <di:waypoint x="1020" y="580" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_8_5_di" bpmnElement="Flow_8_5">
        <di:waypoint x="1160" y="580" />
        <di:waypoint x="1210" y="580" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_8_6_di" bpmnElement="Flow_8_6">
        <di:waypoint x="1350" y="580" />
        <di:waypoint x="1400" y="580" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_8_End_Normal_di" bpmnElement="Flow_8_End_Normal">
        <di:waypoint x="1540" y="580" />
        <di:waypoint x="1592" y="580" />
      </bpmndi:BPMNEdge>
      
      <!-- Message Flows -->
      <bpmndi:BPMNEdge id="MsgFlow_Request_di" bpmnElement="MsgFlow_Request">
        <di:waypoint x="900" y="620" />
        <di:waypoint x="900" y="1000" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="MsgFlow_Response_di" bpmnElement="MsgFlow_Response">
        <di:waypoint x="1090" y="1000" />
        <di:waypoint x="1090" y="620" />
      </bpmndi:BPMNEdge>
      
    </bpmndi:BPMNPlane>
  </bpmndi:BPMNDiagram>
</bpmn:definitions>


