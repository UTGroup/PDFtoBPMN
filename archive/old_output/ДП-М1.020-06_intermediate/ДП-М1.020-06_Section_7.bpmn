  <bpmn:collaboration id="Collaboration_Section_7">
    <bpmn:participant id="Participant_Section_7" name="Раздел 7: Подготовка аудиторов" processRef="Process_Section_7" />
  </bpmn:collaboration>
  
  <bpmn:process id="Process_Section_7" name="Подготовка аудиторов" isExecutable="true">
    
    <bpmn:laneSet id="LaneSet_7">
      <bpmn:lane id="Lane_7_Director" name="Руководитель ДОНО">
        <bpmn:flowNodeRef>Start_7</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_7_1_2</bpmn:flowNodeRef>
      </bpmn:lane>
      
      <bpmn:lane id="Lane_7_Chief" name="Начальник ОООД">
        <bpmn:flowNodeRef>Task_7_3</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_7_5</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_7_7</bpmn:flowNodeRef>
      </bpmn:lane>
      
      <bpmn:lane id="Lane_7_Lead" name="Ведущий менеджер ОООД">
        <bpmn:flowNodeRef>Task_7_4</bpmn:flowNodeRef>
      </bpmn:lane>
      
      <bpmn:lane id="Lane_7_Deputy" name="Зам. ген. директора - СОД">
        <bpmn:flowNodeRef>Task_7_6</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Gateway_7_Review</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>End_7</bpmn:flowNodeRef>
      </bpmn:lane>
    </bpmn:laneSet>
    
    <!-- START -->
    <bpmn:startEvent id="Start_7" name="Актуализация требований">
      <bpmn:outgoing>Flow_7_1</bpmn:outgoing>
    </bpmn:startEvent>
    
    <!-- TASKS -->
    <bpmn:manualTask id="Task_7_1_2" name="Определение квалификационных требований">
      <bpmn:incoming>Flow_7_1</bpmn:incoming>
      <bpmn:outgoing>Flow_7_2</bpmn:outgoing>
    </bpmn:manualTask>
    
    <bpmn:userTask id="Task_7_3" name="Назначение аудиторов из ООНО">
      <bpmn:incoming>Flow_7_2</bpmn:incoming>
      <bpmn:outgoing>Flow_7_3</bpmn:outgoing>
    </bpmn:userTask>
    
    <bpmn:userTask id="Task_7_4" name="Внесение сведений в Реестр">
      <bpmn:incoming>Flow_7_3</bpmn:incoming>
      <bpmn:outgoing>Flow_7_4</bpmn:outgoing>
    </bpmn:userTask>
    
    <bpmn:userTask id="Task_7_5" name="Подготовка итогового Реестра">
      <bpmn:incoming>Flow_7_4</bpmn:incoming>
      <bpmn:incoming>Flow_7_8</bpmn:incoming>
      <bpmn:outgoing>Flow_7_5</bpmn:outgoing>
    </bpmn:userTask>
    
    <bpmn:userTask id="Task_7_6" name="Утверждение Реестра приказом">
      <bpmn:incoming>Flow_7_5</bpmn:incoming>
      <bpmn:outgoing>Flow_7_6</bpmn:outgoing>
    </bpmn:userTask>
    
    <bpmn:exclusiveGateway id="Gateway_7_Review" name="Периодический пересмотр?">
      <bpmn:incoming>Flow_7_6</bpmn:incoming>
      <bpmn:outgoing>Flow_7_7_Yes</bpmn:outgoing>
      <bpmn:outgoing>Flow_7_9_No</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    
    <bpmn:userTask id="Task_7_7" name="Ежегодный пересмотр Реестра">
      <bpmn:incoming>Flow_7_7_Yes</bpmn:incoming>
      <bpmn:outgoing>Flow_7_8</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- END -->
    <bpmn:endEvent id="End_7" name="Реестр утвержден">
      <bpmn:incoming>Flow_7_9_No</bpmn:incoming>
    </bpmn:endEvent>
    
    <!-- FLOWS -->
    <bpmn:sequenceFlow id="Flow_7_1" sourceRef="Start_7" targetRef="Task_7_1_2" />
    <bpmn:sequenceFlow id="Flow_7_2" sourceRef="Task_7_1_2" targetRef="Task_7_3" />
    <bpmn:sequenceFlow id="Flow_7_3" sourceRef="Task_7_3" targetRef="Task_7_4" />
    <bpmn:sequenceFlow id="Flow_7_4" sourceRef="Task_7_4" targetRef="Task_7_5" />
    <bpmn:sequenceFlow id="Flow_7_5" sourceRef="Task_7_5" targetRef="Task_7_6" />
    <bpmn:sequenceFlow id="Flow_7_6" sourceRef="Task_7_6" targetRef="Gateway_7_Review" />
    <bpmn:sequenceFlow id="Flow_7_7_Yes" name="Да (ежегодно)" sourceRef="Gateway_7_Review" targetRef="Task_7_7" />
    <bpmn:sequenceFlow id="Flow_7_8" name="Есть изменения" sourceRef="Task_7_7" targetRef="Task_7_5" />
    <bpmn:sequenceFlow id="Flow_7_9_No" name="Нет" sourceRef="Gateway_7_Review" targetRef="End_7" />
    
  </bpmn:process>
  
