<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL" 
                  xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" 
                  xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" 
                  xmlns:di="http://www.omg.org/spec/DD/20100524/DI" 
                  id="Definitions_Section_10" 
                  targetNamespace="http://bpmn.io/schema/bpmn">
  
  <bpmn:collaboration id="Collaboration_Section_10">
    <bpmn:participant id="Participant_Section_10" name="Раздел 10: Выборочные проверки" processRef="Process_Section_10" />
  </bpmn:collaboration>
  
  <bpmn:process id="Process_Section_10" name="Выборочные проверки" isExecutable="true">
    
    <bpmn:laneSet id="LaneSet_10">
      <bpmn:lane id="Lane_10_Managers" name="Менеджеры ООНО / Начальник ОООД">
        <bpmn:flowNodeRef>Start_10</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_10_1</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Gateway_10_Checklist</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_10_5_Exist</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_10_5_Create</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Gateway_10_Join</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_10_3</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>End_10</bpmn:flowNodeRef>
      </bpmn:lane>
    </bpmn:laneSet>
    
    <!-- START -->
    <bpmn:startEvent id="Start_10" name="Выявлено критическое несоответствие">
      <bpmn:outgoing>Flow_10_1</bpmn:outgoing>
    </bpmn:startEvent>
    
    <!-- TASKS -->
    <bpmn:userTask id="Task_10_1" name="Выявление несоответствий, требующих проверки">
      <bpmn:incoming>Flow_10_1</bpmn:incoming>
      <bpmn:outgoing>Flow_10_2</bpmn:outgoing>
    </bpmn:userTask>
    
    <bpmn:exclusiveGateway id="Gateway_10_Checklist" name="Есть чек-лист?">
      <bpmn:incoming>Flow_10_2</bpmn:incoming>
      <bpmn:outgoing>Flow_10_Exist</bpmn:outgoing>
      <bpmn:outgoing>Flow_10_Create</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    
    <bpmn:userTask id="Task_10_5_Exist" name="Применение существующего чек-листа">
      <bpmn:incoming>Flow_10_Exist</bpmn:incoming>
      <bpmn:outgoing>Flow_10_4</bpmn:outgoing>
    </bpmn:userTask>
    
    <bpmn:userTask id="Task_10_5_Create" name="Составление нового чек-листа">
      <bpmn:incoming>Flow_10_Create</bpmn:incoming>
      <bpmn:outgoing>Flow_10_5</bpmn:outgoing>
    </bpmn:userTask>
    
    <bpmn:exclusiveGateway id="Gateway_10_Join">
      <bpmn:incoming>Flow_10_4</bpmn:incoming>
      <bpmn:incoming>Flow_10_5</bpmn:incoming>
      <bpmn:outgoing>Flow_10_3</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    
    <bpmn:userTask id="Task_10_3" name="Проведение выборочной проверки">
      <bpmn:incoming>Flow_10_3</bpmn:incoming>
      <bpmn:outgoing>Flow_10_6</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- END -->
    <bpmn:endEvent id="End_10" name="Проверка завершена">
      <bpmn:incoming>Flow_10_6</bpmn:incoming>
    </bpmn:endEvent>
    
    <!-- FLOWS -->
    <bpmn:sequenceFlow id="Flow_10_1" sourceRef="Start_10" targetRef="Task_10_1" />
    <bpmn:sequenceFlow id="Flow_10_2" sourceRef="Task_10_1" targetRef="Gateway_10_Checklist" />
    <bpmn:sequenceFlow id="Flow_10_Exist" name="Да" sourceRef="Gateway_10_Checklist" targetRef="Task_10_5_Exist" />
    <bpmn:sequenceFlow id="Flow_10_Create" name="Нет" sourceRef="Gateway_10_Checklist" targetRef="Task_10_5_Create" />
    <bpmn:sequenceFlow id="Flow_10_4" sourceRef="Task_10_5_Exist" targetRef="Gateway_10_Join" />
    <bpmn:sequenceFlow id="Flow_10_5" sourceRef="Task_10_5_Create" targetRef="Gateway_10_Join" />
    <bpmn:sequenceFlow id="Flow_10_3" sourceRef="Gateway_10_Join" targetRef="Task_10_3" />
    <bpmn:sequenceFlow id="Flow_10_6" sourceRef="Task_10_3" targetRef="End_10" />
    
  </bpmn:process>
  
  <!-- VISUALIZATION -->
  <bpmndi:BPMNDiagram id="Diagram_Section_10">
    <bpmndi:BPMNPlane id="Plane_10" bpmnElement="Collaboration_Section_10">
      
      <bpmndi:BPMNShape id="Participant_Section_10_di" bpmnElement="Participant_Section_10" isHorizontal="true">
        <dc:Bounds x="160" y="80" width="1400" height="400" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Lane_10_Managers_di" bpmnElement="Lane_10_Managers" isHorizontal="true">
        <dc:Bounds x="190" y="80" width="1370" height="400" />
      </bpmndi:BPMNShape>
      
      <!-- Start -->
      <bpmndi:BPMNShape id="Start_10_di" bpmnElement="Start_10">
        <dc:Bounds x="252" y="262" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="228" y="305" width="84" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <!-- Tasks -->
      <bpmndi:BPMNShape id="Task_10_1_di" bpmnElement="Task_10_1">
        <dc:Bounds x="340" y="240" width="140" height="80" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Gateway_10_Checklist_di" bpmnElement="Gateway_10_Checklist" isMarkerVisible="true">
        <dc:Bounds x="535" y="255" width="50" height="50" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="518" y="312" width="84" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Task_10_5_Exist_di" bpmnElement="Task_10_5_Exist">
        <dc:Bounds x="640" y="160" width="140" height="80" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Task_10_5_Create_di" bpmnElement="Task_10_5_Create">
        <dc:Bounds x="640" y="320" width="140" height="80" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Gateway_10_Join_di" bpmnElement="Gateway_10_Join" isMarkerVisible="true">
        <dc:Bounds x="835" y="255" width="50" height="50" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="Task_10_3_di" bpmnElement="Task_10_3">
        <dc:Bounds x="940" y="240" width="140" height="80" />
      </bpmndi:BPMNShape>
      
      <bpmndi:BPMNShape id="End_10_di" bpmnElement="End_10">
        <dc:Bounds x="1132" y="262" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="1115" y="305" width="70" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <!-- Edges -->
      <bpmndi:BPMNEdge id="Flow_10_1_di" bpmnElement="Flow_10_1">
        <di:waypoint x="288" y="280" />
        <di:waypoint x="340" y="280" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_10_2_di" bpmnElement="Flow_10_2">
        <di:waypoint x="480" y="280" />
        <di:waypoint x="535" y="280" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_10_Exist_di" bpmnElement="Flow_10_Exist">
        <di:waypoint x="560" y="255" />
        <di:waypoint x="560" y="200" />
        <di:waypoint x="640" y="200" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="568" y="225" width="14" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_10_Create_di" bpmnElement="Flow_10_Create">
        <di:waypoint x="560" y="305" />
        <di:waypoint x="560" y="360" />
        <di:waypoint x="640" y="360" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="566" y="330" width="20" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_10_4_di" bpmnElement="Flow_10_4">
        <di:waypoint x="780" y="200" />
        <di:waypoint x="860" y="200" />
        <di:waypoint x="860" y="255" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_10_5_di" bpmnElement="Flow_10_5">
        <di:waypoint x="780" y="360" />
        <di:waypoint x="860" y="360" />
        <di:waypoint x="860" y="305" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_10_3_di" bpmnElement="Flow_10_3">
        <di:waypoint x="885" y="280" />
        <di:waypoint x="940" y="280" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="Flow_10_6_di" bpmnElement="Flow_10_6">
        <di:waypoint x="1080" y="280" />
        <di:waypoint x="1132" y="280" />
      </bpmndi:BPMNEdge>
      
    </bpmndi:BPMNPlane>
  </bpmndi:BPMNDiagram>
</bpmn:definitions>


