<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL" 
                  xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" 
                  xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" 
                  xmlns:di="http://www.omg.org/spec/DD/20100524/DI" 
                  id="Definitions_Section_11" 
                  targetNamespace="http://bpmn.io/schema/bpmn">
  
  <bpmn:collaboration id="Collaboration_Section_11">
    <bpmn:participant id="Participant_Section_11" name="Раздел 11: Плановые аудиты" processRef="Process_Section_11" />
    <bpmn:participant id="Participant_Agent" name="Агент (внешний)" />
    <bpmn:messageFlow id="MsgFlow_Plan_to_Agent" sourceRef="Task_11_3_1_5" targetRef="Participant_Agent" />
    <bpmn:messageFlow id="MsgFlow_Report_to_Agent" sourceRef="Task_11_3_5" targetRef="Participant_Agent" />
  </bpmn:collaboration>
  
  <bpmn:process id="Process_Section_11" name="Плановые аудиты" isExecutable="true">
    
    <bpmn:laneSet id="LaneSet_11">
      <bpmn:lane id="Lane_11_Management" name="Руководство ОООД/ООНО">
        <bpmn:flowNodeRef>Start_11</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_11_2_1</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_11_2_4</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_11_3_1_1</bpmn:flowNodeRef>
      </bpmn:lane>
      
      <bpmn:lane id="Lane_11_Deputy" name="Зам. ген. директора - СОД">
        <bpmn:flowNodeRef>Task_11_2_4_Approve</bpmn:flowNodeRef>
      </bpmn:lane>
      
      <bpmn:lane id="Lane_11_Audit_Leader" name="Руководитель аудиторской группы">
        <bpmn:flowNodeRef>Task_11_3_1_4</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_11_3_1_5</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_11_3_4</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_11_3_5</bpmn:flowNodeRef>
      </bpmn:lane>
      
      <bpmn:lane id="Lane_11_Auditors" name="Аудиторы">
        <bpmn:flowNodeRef>Task_11_3_2</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_11_3_3_4</bpmn:flowNodeRef>
      </bpmn:lane>
      
      <bpmn:lane id="Lane_11_Control" name="Контроль выполнения">
        <bpmn:flowNodeRef>Task_11_3_8</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>End_11</bpmn:flowNodeRef>
      </bpmn:lane>
    </bpmn:laneSet>
    
    <!-- START -->
    <bpmn:startEvent id="Start_11" name="Начало планового аудита">
      <bpmn:outgoing>Flow_11_1</bpmn:outgoing>
    </bpmn:startEvent>
    
    <!-- 11.2 ПЛАНИРОВАНИЕ -->
    <bpmn:userTask id="Task_11_2_1" name="Подготовка годовой программы мониторинга">
      <bpmn:incoming>Flow_11_1</bpmn:incoming>
      <bpmn:outgoing>Flow_11_2</bpmn:outgoing>
    </bpmn:userTask>
    
    <bpmn:userTask id="Task_11_2_4" name="Заполнение программы (даты, города, агенты)">
      <bpmn:incoming>Flow_11_2</bpmn:incoming>
      <bpmn:outgoing>Flow_11_3</bpmn:outgoing>
    </bpmn:userTask>
    
    <bpmn:userTask id="Task_11_2_4_Approve" name="Утверждение программы">
      <bpmn:incoming>Flow_11_3</bpmn:incoming>
      <bpmn:outgoing>Flow_11_4</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- 11.3.1 ПОДГОТОВКА К АУДИТУ -->
    <bpmn:userTask id="Task_11_3_1_1" name="Определение объема и назначение аудиторов">
      <bpmn:incoming>Flow_11_4</bpmn:incoming>
      <bpmn:outgoing>Flow_11_5</bpmn:outgoing>
    </bpmn:userTask>
    
    <bpmn:userTask id="Task_11_3_1_4" name="Составление плана аудита">
      <bpmn:incoming>Flow_11_5</bpmn:incoming>
      <bpmn:outgoing>Flow_11_6</bpmn:outgoing>
    </bpmn:userTask>
    
    <bpmn:sendTask id="Task_11_3_1_5" name="Направление плана агенту (за 10 дней)">
      <bpmn:incoming>Flow_11_6</bpmn:incoming>
      <bpmn:outgoing>Flow_11_7</bpmn:outgoing>
    </bpmn:sendTask>
    
    <!-- 11.3.2-11.3.3 ПРОВЕДЕНИЕ АУДИТА -->
    <bpmn:userTask id="Task_11_3_2" name="Проведение аудита на месте">
      <bpmn:incoming>Flow_11_7</bpmn:incoming>
      <bpmn:outgoing>Flow_11_8</bpmn:outgoing>
    </bpmn:userTask>
    
    <bpmn:userTask id="Task_11_3_3_4" name="Заполнение чек-листа проверки">
      <bpmn:incoming>Flow_11_8</bpmn:incoming>
      <bpmn:outgoing>Flow_11_9</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- 11.3.4-11.3.6 ОТЧЕТНОСТЬ -->
    <bpmn:userTask id="Task_11_3_4" name="Подготовка отчета (7 дней)">
      <bpmn:incoming>Flow_11_9</bpmn:incoming>
      <bpmn:outgoing>Flow_11_10</bpmn:outgoing>
    </bpmn:userTask>
    
    <bpmn:sendTask id="Task_11_3_5" name="Направление отчета агенту и ДУБП">
      <bpmn:incoming>Flow_11_10</bpmn:incoming>
      <bpmn:outgoing>Flow_11_11</bpmn:outgoing>
    </bpmn:sendTask>
    
    <!-- 11.3.7-11.3.8 КОРРЕКТИРУЮЩИЕ ДЕЙСТВИЯ -->
    <bpmn:userTask id="Task_11_3_8" name="Контроль выполнения корректирующих действий">
      <bpmn:incoming>Flow_11_11</bpmn:incoming>
      <bpmn:outgoing>Flow_11_12</bpmn:outgoing>
    </bpmn:userTask>
    
    <!-- END -->
    <bpmn:endEvent id="End_11" name="Аудит завершен">
      <bpmn:incoming>Flow_11_12</bpmn:incoming>
    </bpmn:endEvent>
    
    <!-- FLOWS -->
    <bpmn:sequenceFlow id="Flow_11_1" sourceRef="Start_11" targetRef="Task_11_2_1" />
    <bpmn:sequenceFlow id="Flow_11_2" sourceRef="Task_11_2_1" targetRef="Task_11_2_4" />
    <bpmn:sequenceFlow id="Flow_11_3" sourceRef="Task_11_2_4" targetRef="Task_11_2_4_Approve" />
    <bpmn:sequenceFlow id="Flow_11_4" sourceRef="Task_11_2_4_Approve" targetRef="Task_11_3_1_1" />
    <bpmn:sequenceFlow id="Flow_11_5" sourceRef="Task_11_3_1_1" targetRef="Task_11_3_1_4" />
    <bpmn:sequenceFlow id="Flow_11_6" sourceRef="Task_11_3_1_4" targetRef="Task_11_3_1_5" />
    <bpmn:sequenceFlow id="Flow_11_7" sourceRef="Task_11_3_1_5" targetRef="Task_11_3_2" />
    <bpmn:sequenceFlow id="Flow_11_8" sourceRef="Task_11_3_2" targetRef="Task_11_3_3_4" />
    <bpmn:sequenceFlow id="Flow_11_9" sourceRef="Task_11_3_3_4" targetRef="Task_11_3_4" />
    <bpmn:sequenceFlow id="Flow_11_10" sourceRef="Task_11_3_4" targetRef="Task_11_3_5" />
    <bpmn:sequenceFlow id="Flow_11_11" sourceRef="Task_11_3_5" targetRef="Task_11_3_8" />
    <bpmn:sequenceFlow id="Flow_11_12" sourceRef="Task_11_3_8" targetRef="End_11" />
    
  </bpmn:process>
  
